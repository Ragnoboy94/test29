    },
    "components": {
        "schemas": {
            "Brand": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Toyota"
                    }
                },
                "type": "object"
            },
            "CarModel": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 10
                    },
                    "name": {
                        "type": "string",
                        "example": "Corolla"
                    },
                    "brand_id": {
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            },
            "Car": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 100
                    },
                    "brand_id": {
                        "type": "integer",
                        "example": 1
                    },
                    "car_model_id": {
                        "type": "integer",
                        "example": 10
                    },
                    "year": {
                        "type": "integer",
                        "example": 2020,
                        "nullable": true
                    },
                    "mileage": {
                        "type": "integer",
                        "example": 12000,
                        "nullable": true
                    },
                    "color": {
                        "type": "string",
                        "example": "black",
                        "nullable": true
                    },
                    "user_id": {
                        "type": "integer",
                        "example": 5,
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "CarCreate": {
                "required": [
                    "brand_id",
                    "car_model_id"
                ],
                "properties": {
                    "brand_id": {
                        "type": "integer",
                        "example": 1
                    },
                    "car_model_id": {
                        "type": "integer",
                        "example": 10
                    },
                    "year": {
                        "type": "integer",
                        "example": 2020
                    },
                    "mileage": {
                        "type": "integer",
                        "example": 12000
                    },
                    "color": {
                        "type": "string",
                        "example": "black"
                    }
                },
                "type": "object"
            },
            "LoginRequest": {
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "test@example.com"
                    },
                    "password": {
                        "type": "string",
                        "example": "password"
                    }
                },
                "type": "object"
            },
            "TokenResponse": {
                "properties": {
                    "token": {
                        "type": "string",
                        "example": "1|abcd..."
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "Token",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Auth",
            "description": "Auth"
        },
        {
            "name": "Brands",
            "description": "Brands"
        },
        {
            "name": "Cars",
            "description": "Cars"
        },
        {
            "name": "Models",
            "description": "Models"
        }
    ]
